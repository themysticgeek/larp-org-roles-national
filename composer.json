{
    "name": "themysticgeek/larp-org-roles-national",
    "description": "Setting up roles above local/game: Example - Regional / National / Genre",
    "keywords": [
        "themysticgeek",
        "laravel",
        "larp-org-roles-national"
    ],
    "homepage": "https://github.com/themysticgeek/larp-org-roles-national",
    "license": "MIT",
    "authors": [
        {
            "name": "Jessica Karels (The Mystic Geek)",
            "email": "jess@themysticgeek.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Themysticgeek\\LarpOrgRolesNational\\": "src",
            "Themysticgeek\\LarpOrgRolesNational\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Themysticgeek\\LarpOrgRolesNational\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Themysticgeek\\LarpOrgRolesNational\\LarpOrgRolesNationalServiceProvider"
            ],
            "aliases": {
                "LarpOrgRolesNational": "Themysticgeek\\LarpOrgRolesNational\\LarpOrgRolesNationalFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
